{
  "workDir": ".",
  "dependencies": [
    {
      "name": "privatesky",
      "src": "http://github.com/privatesky/privatesky.git",
      "actions": [
        {
          "type": "smartClone",
          "target": ".",
          "collectLog": false
        },
        {
          "type": "execute",
          "cmd": "cd privatesky && npm install"
        }
      ]
    },
    {
      "name": "build privatesky",
      "actions": [
        {
          "type": "execute",
          "cmd": "cd privatesky && npm run build"
        }
      ]
    }
  ],
  "build": [

  ]
}
